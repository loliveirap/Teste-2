
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <div class="well-block">
                <div class="well-title">
                    <h2>Login</h2>
                </div>
                <form id="formCategoria" method="post" enctype="application/x-www-form-urlencoded">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label" for="email">Usuario/Email</label>
                                <input id="email" name="email" type="text" class="form-control input-md">
                            </div>

                            <div class="form-group">
                                <label class="control-label" for="senha">Senha</label>
                                <input id="senha" name="senha" type="password" class="form-control input-md">
                            </div>

                            <div class="form-group">
                                <button type="submit" id="btoLogin" name="btoLogin" class="btn btn-default">Entrar</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>



